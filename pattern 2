//In the below pattern columns are started from 1 to N, and the number of columns is equal to the number of rows.
#include<stdio.h>
int main()
{
   int n;

   printf("Enter the number of rows: ");
   scanf("%d",&n);

   for(int r=1; r<=n; r++)
   {
     for(int c=1; c<=n; c++)
     {
       printf("%3d",c);
     }
     printf("\n");
   }

   return 0;
}
